{
  "name": "Booking",
  "plural": "bookings",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "inDraft": {
      "type": "boolean",
      "default": false
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "hotelId": {
      "type": "string",
      "required": true
    },
    "numberOfNights": {
      "type": "number",
      "required": true
    },
    "numberOfRooms": {
      "type": "number",
      "required": true
    },
    "date": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "hotel": {
      "type": "belongsTo",
      "model": "Hotel",
      "property": "hotelId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "property": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}